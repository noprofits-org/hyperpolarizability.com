@article{Alloatti:11, 
author = {L. Alloatti and D. Korn and R. Palmer and D. Hillerkuss and J. Li and A. Barklund and R. Dinu and J. Wieland and M. Fournier and J. Fedeli and H. Yu and W. Bogaerts and P. Dumon and R. Baets and C. Koos and W. Freude and J. Leuthold}, 
