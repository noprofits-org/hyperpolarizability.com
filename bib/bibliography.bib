@article{doi:10.1021/jp962735p,
author = {O. K. Song, and J. N. Woodford, and and C. H. Wang},
title = {Effects of Two-Photon Fluorescence and Polymerization on the First Hyperpolarizability of an Azobenzene Dye},
journal = {The Journal of Physical Chemistry A},
volume = {101},
number = {18},
pages = {3222-3226},
year = {1997},
doi = {10.1021/jp962735p},

URL = { 
        http://dx.doi.org/10.1021/jp962735p
    
},
eprint = { 
        http://dx.doi.org/10.1021/jp962735p
    
}
,
    abstract = { The technique of hyper-Rayleigh scattering (HRS) has been employed to investigate the first hyperpolarizability ($\beta$) of the azo dye Disperse Red 19 and a polymer in which Disperse Red 19 appears as a side-chain moiety. In addition to HRS, these two molecules were found to exhibit substantial two-photon-induced fluorescence (TPF). The TPF spectra for these molecules were measured, and corrected HRS intensities were obtained. Using an external reference method, $\beta$ values of (1480 $\pm$ 100) $\times$ $10^{-30}$ and (1470 $\pm$ 90) $\times$ $10^{-30}$ esu were determined for the dye and polymer systems, respectively, in dimethyl sulfoxide at 1064 nm. The difference between these two values is very small and shows that the effect of polymerization on the HRS intensity, in the dilute concentration limit, is negligible. }
}






@article{doi:10.1021/jp9639861,
author = {J. N. Woodford, and M. A. Pauley, and and C. H. Wang},
title = {Solvent Dependence of the First Molecular Hyperpolarizability of p-Nitroaniline Revisited},
journal = {The Journal of Physical Chemistry A},
volume = {101},
number = {11},
pages = {1989-1992},
year = {1997},
doi = {10.1021/jp9639861},

URL = { 
        http://dx.doi.org/10.1021/jp9639861
    
},
eprint = { 
        http://dx.doi.org/10.1021/jp9639861
    
}
,
    abstract = { The solvent effect on the molecular first hyperpolarizability, $\beta$, of p-nitroaniline (pNA) has been investigated. Using the hyper-Rayleigh scattering technique, we have also determined $\beta$ of pNA in DMSO using 1064 nm laser radiation. This $\beta$ value has not been previously reported. With the help of Onsager's theory, we have arrived at a correction factor to account for the solvent dependence of the values of the intrinsic hyperpolarizability $\beta$0. The Onsager model significantly removes the strong solvent dependence of $\beta$ (or $\beta$0) on the dielectric constant. }
}

