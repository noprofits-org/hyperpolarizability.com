title = {High speed plasmonic modulator array enabling dense optical interconnect solutions}, 
volume = {23}, 
month = {Nov},
year = {2015},
url = {http://www.opticsexpress.org/abstract.cfm?URI=oe-23-23-29746},
doi = {10.1364/OE.23.029746},
abstract = {Plasmonic modulators might pave the way for a new generation of compact low-power high-speed optoelectronic devices. We introduce an extremely compact transmitter based on plasmonic Mach-Zehnder modulators offering a capacity of 4 \&\#x00D7; 36 Gbit/s on a footprint that is only limited by the size of the high-speed contact pads. The transmitter array is contacted through a multicore fiber with a channel spacing of 50 \&\#x03BC;m.},
}




@article{Melikyan:15, 
author = {A. Melikyan and K. Koehnle and M. Lauermann and R. Palmer and S. Koeber and S. Muehlbrandt and P. C. Schindler and D. L. Elder and S. Wolf and W. Heni and C. Haffner and Y. Fedoryshyn and D. Hillerkuss and M. Sommer and L. R. Dalton and D. Van Thourhout and W. Freude and M. Kohl and J. Leuthold and C. Koos}, 
journal = {Opt. Express}, 
keywords = {Surface plasmons; Plasmonics; Modulators},
number = {8}, 
pages = {9938--9946}, 
publisher = {OSA},
title = {Plasmonic-organic hybrid (POH) modulators for OOK and BPSK signaling at 40 Gbit/s}, 
volume = {23}, 
month = {Apr},
year = {2015},
url = {http://www.opticsexpress.org/abstract.cfm?URI=oe-23-8-9938},
doi = {10.1364/OE.23.009938},
abstract = {We report on high-speed plasmonic-organic hybrid Mach-Zehnder modulators comprising ultra-compact phase shifters with lengths as small as 19 {\textmu}m. Choosing an optimum phase shifter length of 29 {\textmu}m, we demonstrate 40 Gbit/s on-off keying (OOK) modulation with direct detection and a BER \&lt; 6 {\texttimes} 10{\textminus}4. Furthermore, we report on a 29 {\textmu}m long binary-phase shift keying (BPSK) modulator and show that it operates error-free (BER \&lt; 1 {\texttimes} 10{\textminus}10) at data rates up to 40 Gbit/s and with an energy consumption of 70 fJ/bit.},
}



